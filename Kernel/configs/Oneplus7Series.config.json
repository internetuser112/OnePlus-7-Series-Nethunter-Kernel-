[
  {
    "kernelSource": {
      "name": "Op7Series_KSU_Nethunter",
      "repo": "https://github.com/0wnerDied/Neptune_kernel_sm8150_oneplus",
      "branch": "fifteen",
      "device": "Oneplus7Series",
      "defconfig": "gulch_defconfig"
    },
    "toolchains": [
      {
        "repo": "https://gitlab.com/internetuser111/prebuilts_clang_host_linux-x86",
        "branch": "fourteen",
        "name": "clang",
        "binaryEnv": [
          "clang-r522817/bin"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
        "LD": "ld.lld",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "READELF": "llvm-readelf",
        "OBJSIZE": "llvm-size",
        "STRIP": "llvm-strip",
        "LDGOLD": "aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "llvm-ar",
        "LLVM_DIS": "llvm-dis",
        "CONFIG_THINLTO": "y"
      }
    },
    "AnyKernel3": {
        "use": true,
        "release": true,
        "custom": {
          "repo": "https://github.com/internetuser111/AnyKernel3",
          "branch": "op7-nh"
        }
      },
      "enableKernelSU": true,
      "enableLXC": true
    }
]
